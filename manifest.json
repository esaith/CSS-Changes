{
  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  },
  "name": "CSS Changes",
  "browser_action": {
    "default_popup": "browserAction.html"
  },
  "version": "0.1",
  "description": "Get CSS changes",
  "devtools_page": "devtools.html",
  "permissions": [
    "background",
    "storage",
    "webNavigation",
    "tabs",
    "http://*/",
    "https://*/",
    "<all_urls>"
  ],
  "manifest_version": 2
}